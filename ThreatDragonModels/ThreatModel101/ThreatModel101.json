              "threats": [
                {
                  "id": "7e5381f3-083b-4655-9b4c-7aadc3228eef",
                  "title": "Access token theft",
                  "status": "Mitigated",
                  "severity": "High",
                  "type": "Information disclosure",
                  "description": "Access token stored in browser can be impersonated by attacker and  stolen\n",
                  "mitigation": "1.  Key cloak configurations in place to generate 'short lived tokens'\n2.  Enforcing CSP on AW Gateway to allow only valid endpoints\n3.  Key cloak tokens are stored in Session Storage and hence cleared during Tab close.\n",
                  "modelType": "default",
                  "new": false,
                  "number": 6,
                  "score": ""
                },
                {
                  "id": "3406d470-ecfa-4d6f-ab60-6af3ba6fdb30",
                  "title": "Refresh Token Abuse",
                  "status": "Mitigated",
                  "severity": "High",
                  "type": "Spoofing",
                  "description": "An attacker steals a valid refresh token (via XSS, malware, browser storage access etc..) and uses it to continuously create new access tokens, even after the original access token expires.",
                  "mitigation": "1. Keycloak configurations in place to generate short lived access tokens\n2. Enabled 'Revoke Refresh Token' option at Keycloak, to discard refresh token after single use (settings at Keycloak).",
                  "modelType": "default",
                  "new": false,
                  "number": 19,
                  "score": ""
                },
                {
                  "id": "eb7fa5fe-396f-465c-9aee-7a81e1912473",
                  "title": "Token Replay",
                  "status": "Mitigated",
                  "severity": "Medium",
                  "type": "Spoofing",
                  "description": "A stolen access token is reused by an attacker to impersonate a legitimate client.\n",
                  "mitigation": "1. AW to use OIDC client's \"nonce\" feature to validate the access token at the reciever end.\n2. Enabled short-lived access tokens from keycloak\n3. Enabled \"aud=signalling\" specifying the audience of this token.",
                  "modelType": "default",
                  "new": false,
                  "number": 22,
                  "score": ""
                }
              ]
            }
